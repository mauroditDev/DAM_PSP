package ut2p1;

public class HiloReloj {
	public static void main(String[] args){
		try{
			for (int i = 0; i<100; i++){

				HiloTic tic = new HiloTic();
				HiloTac tac = new HiloTac();
				tic.start();
				tic.wait();
				tic.notify();
				tac.start();
				tac.wait();
				tac.notify();
				
			}
		}catch(Exception e){
			e.printStackTrace();
		}
	}

}
