package ut2p3;

public class PruebaJoin {

	public static void main(String[] args){
		try{
			int k = 0;
			HiloJoin c1 = new HiloJoin(++k);
			HiloJoin c2 = new HiloJoin(++k);
			HiloJoin c3 = new HiloJoin(++k);
			HiloJoin c4 = new HiloJoin(++k);
			HiloJoin c5 = new HiloJoin(++k);
			
			c1.start();
			c1.join();
			c2.start();
			c2.join();
			c3.start();
			c3.join();
			c4.start();
			c4.join();
			c5.start();
			c5.join();
			
			System.out.println("Todos los hilos creados");
		}catch(Exception e){}
	}
	
}
