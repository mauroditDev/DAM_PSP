package ut2p2;

public class HiloReloj {
	public static void main(String[] args){
		try{
			for (int i = 0; i<100; i++){

				HiloTicR tic = new HiloTicR();
				HiloTacR tac = new HiloTacR();
				tic.start();
				Thread.sleep(1000);
				tac.start();
				Thread.sleep(1000);
			}
		}catch(Exception e){}
	}

}
