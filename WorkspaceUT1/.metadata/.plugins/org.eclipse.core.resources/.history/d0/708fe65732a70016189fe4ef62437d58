package ut2p3;

public class PruebaJoin {

	public static void main(String[] args){
		try{
			int k = 0;
			HiloJoin c[];
			while(k<6){
				c[k] = new HiloJoin(++k);
			}
			k=0;
			while(k<6){
				c[k].start();
				c[k].join();
				k++;
			}
			
			System.out.println("Todos los hilos creados");
		}catch(Exception e){}
	}
	
}
