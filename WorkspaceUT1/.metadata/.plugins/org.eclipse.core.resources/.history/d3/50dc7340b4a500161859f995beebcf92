package ut2p3;

public class VariosHilos {

	public static void main(String[] args){
		int k = 0;
		HiloSleep c1 = new HiloSleep(++k);
		HiloSleep c2 = new HiloSleep(++k);
		HiloSleep c3 = new HiloSleep(++k);
		HiloSleep c4 = new HiloSleep(++k);
		HiloSleep c5 = new HiloSleep(++k);
		
		c1.start();
		c2.start();
		c3.start();
		c4.start();
		c5.start();
		
		System.out.println("Todos los hilos creados");
		
	}
	
}
