package ut2p2;

public class VariosHilos {

	public static void main(String[] args){
		int k = 0;
		HiloCR c1 = new HiloCR(++k);
		HiloCR c2 = new HiloCR(++k);
		HiloCR c3 = new HiloCR(++k);
		HiloCR c4 = new HiloCR(++k);
		HiloCR c5 = new HiloCR(++k);
		
		c1.start();
		c2.start();
		c3.start();
		c4.start();
		c5.start();
		
		System.out.println("Todos los hilos creados");
		
	}
	
}
