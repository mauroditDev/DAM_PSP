package ut2p1;

import java.io.FileWriter;
import java.io.PrintWriter;

public class HiloFor extends Thread{
	
	public void run(){
		try{
			PrintWriter pw = new PrintWriter(new FileWriter("texto.txt"));
			for(int i = 0; i< 10000; i++)
				pw.write("estoy dentro del hilo");
		}catch(Exception e){}
		
	}

}
